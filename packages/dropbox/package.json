{
  "name": "@node-api-toolkit/dropbox",
  "scripts": {
    "start": "ts-node .",
    "test": "jest --config jest.config.json --runInBand",
    "test:watch": "yarn test --watch",
    "build": "tsc --verbose --build",
    "create-dropbox-token-file": "generate-oauth-token-cli --oauthStrategy passport-dropbox-oauth2 --oauthStrategyOptions.apiVersion 2 --saveTokenToFile --appKey=$DROPBOX_APP_KEY --appSecret=$DROPBOX_APP_SECRET",
    "create-jest-dropbox-token-file": "create-dropbox-token-file --tokenId=JEST_NODE_API_TOOLKIT_DROPBOX_PROTOTYPE_TOKEN"
  },
  "main": "dist/index.js",
  "version": "0.9.0",
  "publishConfig": {
    "access": "public"
  },
  "license": "GPL-3.0",
  "public": true,
  "devDependencies": {
    "@types/jest": "^25.1.2",
    "@types/node": "^13.7.1",
    "express": "^4.17.1",
    "jest": "^25.1.0",
    "nock": "^12.0.1",
    "ts-jest": "^25.2.0",
    "ts-node": "^8.6.2",
    "tsc": "^1.20150623.0",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "@node-cli-toolkit/generate-oauth-token-cli": "^0.5.2",
    "axios": "^0.19.2",
    "passport": "^0.4.1",
    "passport-dropbox-oauth2": "^1.1.0"
  }
}
